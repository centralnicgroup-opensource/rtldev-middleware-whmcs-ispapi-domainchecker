<div class="item" data-category="{{id}}" data-name="{{name}}">
    <div class="item-content">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <span class="dropcat glyphicon glyphicon-remove"></span> {{name}}
            </div>
            <div class="panel-body">
                <div class="tldgrid" data-category="{{id}}">
                {{#tlds}}
                    {{ >tldgriditem}}
                {{/tlds}}
                </div>
            </div>
        </div>
    </div>
</div>